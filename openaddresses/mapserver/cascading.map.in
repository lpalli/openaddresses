MAP
 NAME "cascading"
 IMAGECOLOR 255 255 255
 IMAGETYPE png
 STATUS ON
 UNITS METERS
 MAXSIZE 4000
 TRANSPARENT on
 CONFIG "PROJ_LIB" "%PROJ_LIB%"
 PROJECTION
    "init=epsg:900913"
 END
 DEBUG 3
 CONFIG "MS_ERRORFILE" "%MS_ERRORFILE%"

 WEB
   IMAGEPATH "/tmp"
   IMAGEURL "/"
   METADATA
     "wms_title"           "Cascading"
     "wms_onlineresource"  "%BASE_WMS%"
     "wms_srs"             "EPSG:900913"
   END
 END

 SYMBOL
    NAME 'circle'
    TYPE ELLIPSE
    FILLED TRUE
    POINTS
        1 1
    END
 END

 SYMBOL
    NAME 'square'
    TYPE VECTOR
    FILLED TRUE
    POINTS
        0 1
        0 0
        1 0
        1 1
        0 1
    END
 END

 SYMBOL
   NAME "triangle"
   TYPE vector
   FILLED true
   POINTS
     0 4
     2 0
     4 4
     0 4
   END
 END


 LAYER
  NAME "prov_bound"
  TYPE RASTER
  STATUS ON
  CONNECTION "http://www2.dmsolutions.ca/cgi-bin/mswms_gmap?"
  CONNECTIONTYPE WMS
  METADATA
    "wms_srs"             "EPSG:42304"
    "wms_name"            "prov_bound"
    "wms_server_version"  "1.1.1"
    "wms_format"          "image/gif"
  END
 END

 LAYER
  NAME "SITN_ortho"
  TYPE RASTER
  STATUS ON
  CONNECTION "http://sitn.ne.ch/ogc-sitn-open/wms?"
  CONNECTIONTYPE WMS
  METADATA
    "wms_srs"             "EPSG:4326"
    "wms_name"            "ortho"
    "wms_server_version"  "1.1.1"
    "wms_format"          "image/jpeg"
  END
 END

 LAYER
  NAME "GENEVE_ortho"
  TYPE RASTER
  STATUS ON
  CONNECTION "http://etat.geneve.ch/ags2/services/Orthophotos_2005/MapServer/WMSServer"
  CONNECTIONTYPE WMS
  METADATA
    "wms_srs"             "EPSG:4326"
    "wms_name"            "0"
    "wms_server_version"  "1.1.1"
    "wms_format"          "image/jpeg"
  END
 END

 LAYER
  NAME "adv_dop"
  TYPE RASTER
  STATUS ON
  CONNECTION "http://www.landesvermessung.sachsen.de/ias/basiskarte4/service/SRV4ADV_P_DOPRGB/WMSFREE_TK/wmsservice"
  CONNECTIONTYPE WMS
  METADATA
    "wms_srs"             "EPSG:4326"
    "wms_name"            "0"
    "wms_server_version"  "1.1.1"
    "wms_format"          "image/jpeg"
  END
 END

 LAYER
  NAME "lausanne_epfl"
  TYPE RASTER
  STATUS ON
  CONNECTION "http://plan.epfl.ch/lausanne-epfl-osm"
  CONNECTIONTYPE WMS
  METADATA
    "wms_srs"             "EPSG:4326"
    "wms_name"            "lausanne,epfl"
    "wms_server_version"  "1.1.1"
    "wms_format"          "image/jpeg"
  END
 END


 LAYER
   NAME "address"
   TYPE POINT
   CONNECTIONTYPE POSTGIS
   CONNECTION "%DATABASE_CONNECTION_ADDRESS%"
   PROJECTION
       "init=epsg:4326"
   END
   DATA "geom from address using unique id"
   STATUS ON
   CLASS
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 12
         MAXSCALEDENOM 2500
         OFFSET -1 -1
      END
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 8
         MINSCALEDENOM 2500
         MAXSCALEDENOM 7500
         OFFSET -1 -1
      END
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 4
         MINSCALEDENOM 7500
         MAXSCALEDENOM 1000000000
         OFFSET -1 -1
      END
   END
   METADATA
     "ows_title" "address"
     "wms_srs" "EPSG:900913"
   END
 END

END
