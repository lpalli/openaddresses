 LAYER
   NAME "address"
   TYPE POINT
   CONNECTIONTYPE POSTGIS
   CONNECTION "%DATABASE_CONNECTION_ADDRESS%"
   PROCESSING "CLOSE_CONNECTION=DEFER"
   PROJECTION
       "init=epsg:4326"
   END
   DATA "geom from address using unique id using srid=4326"
   STATUS ON
   CLASS
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 12
         MAXSCALEDENOM 2500
         OFFSET -1 -1
      END
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 8
         MINSCALEDENOM 2500
         MAXSCALEDENOM 7500
         OFFSET -1 -1
      END
      STYLE
         SYMBOL "circle"
         COLOR 255 0 0
         SIZE 4
         MINSCALEDENOM 7500
         MAXSCALEDENOM 1000000000
         OFFSET -1 -1
      END
   END
   METADATA
     "ows_title" "address"
     "wms_srs" "EPSG:900913"
   END
 END

 LAYER
   NAME "addressNumber"
   TYPE POINT
   CONNECTIONTYPE POSTGIS
   LABELITEM "housenumber"
   LABELCACHE ON
   CONNECTION "%DATABASE_CONNECTION_ADDRESS%"
   PROCESSING "CLOSE_CONNECTION=DEFER"
   PROJECTION
       "init=epsg:4326"
   END
   DATA "geom from address using unique id using srid=4326"
   STATUS ON
   CLASS
      MAXSCALEDENOM 5000
      STYLE
         SYMBOL "circle"
         COLOR 254 254 254
         SIZE 18
         OFFSET -1 -1
      END
      LABEL
        FONT "vera-sans"
        TYPE truetype
        POSITION cc
        SIZE 6
        COLOR 0 0 0
      END
   END
   METADATA
     "ows_title" "addressNumber"
     "wms_srs" "EPSG:900913"
   END
 END

