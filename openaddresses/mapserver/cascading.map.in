MAP
 NAME "cascading"
 IMAGECOLOR 255 255 255
 IMAGETYPE png
 STATUS ON
 UNITS METERS
 MAXSIZE 4000
 TRANSPARENT on
 CONFIG "PROJ_LIB" "%PROJ_LIB%"
 PROJECTION
    "init=epsg:900913"
 END
 FONTSET "fonts.txt"
 
 DEBUG 3
 CONFIG "MS_ERRORFILE" "%MS_ERRORFILE%"

 WEB
   IMAGEPATH "/tmp"
   IMAGEURL "/"
   METADATA
     "wms_title"           "Cascading"
     "wms_onlineresource"  "%BASE_WMS%"
     "wms_srs"             "EPSG:900913"
   END
 END

 SYMBOL
    NAME 'circle'
    TYPE ELLIPSE
    FILLED TRUE
    POINTS
        1 1
    END
 END

 SYMBOL
    NAME 'square'
    TYPE VECTOR
    FILLED TRUE
    POINTS
        0 1
        0 0
        1 0
        1 1
        0 1
    END
 END

 SYMBOL
   NAME "triangle"
   TYPE vector
   FILLED true
   POINTS
     0 4
     2 0
     4 4
     0 4
   END
 END


